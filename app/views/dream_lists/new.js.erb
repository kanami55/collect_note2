let new_modal = document.getElementById('new_modal');
let back_mask = document.getElementById('back_mask');
new_modal.innerHTML = '<%= escape_javascript(render "dream_lists/new", dream_list: @dream_list) %>';
new_modal.classList.remove('new_hidden');
back_mask.classList.remove('new_hidden');
back_mask.addEventListener('click', () => {
  new_modal.classList.add('new_hidden');
  back_mask.classList.add('new_hidden');
});